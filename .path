# Load GNU coreutils into PATH
export PATH="/usr/local/opt/coreutils/libexec/gnubin:$PATH"
export PATH="/usr/local/opt/gnu-sed/libexec/gnubin:$PATH"
export PATH="/usr/local/opt/grep/libexec/gnubin:$PATH"

# Discover GNU coreutils man pages
export MANPATH="/usr/local/opt/coreutils/libexec/man:$MANPATH"
export MANPATH="/usr/local/opt/gnu-sed/libexec/man:$MANPATH"
export MANPATH="/usr/local/opt/grep/libexec/man:$MANPATH"

# Load binaries globally installed via Composer
export PATH="$PATH:$HOME/.composer/vendor/bin"

# Add `$(brew --prefix)/bin` and `$(brew --prefix)/sbin` to the `$PATH`
export PATH="$(brew --prefix)/bin:$(brew --prefix)/sbin:$PATH"

# Add Yarn and npm global binaries to the `$PATH`
export PATH="$HOME/.config/yarn/global/node_modules/.bin:$(npm --global prefix)/bin:$PATH"

# Add rbenv to PATH
export PATH="$HOME/.rbenv/bin:$PATH"

# Add Go-installed binaries to PATH
export PATH=$PATH:/usr/local/opt/go/libexec/bin

export PATH=$PATH:$HOME/go/bin

export PATH=$PATH:$HOME/Library/Python/3.7/bin

# Add `~/bin` to the `$PATH`
export PATH="$HOME/bin:$PATH";
export PATH="/usr/local/opt/openssl@1.1/bin:$PATH"
